int findCeil(Node* root, int input) 
{
    if (root == NULL) return -1;
    int greaterEqual=-1;
    while(root!=nullptr)
    {
        if(root->data==input)
         return root->data;
        else if(root->data<input)
         root=root->right;
        else
        {
            greaterEqual=root->data;
            root=root->left;
        }
    }
    return greaterEqual;
}
