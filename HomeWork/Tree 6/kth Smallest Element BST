class Solution {
public:
    void inorder(TreeNode *root, int &ans, int &k)
    {
       if(root==nullptr || k==0)
        return;
       if(k>0)
        inorder(root->left, ans, k); 
       if(k>0)
       {
         ans=root->val;
         k--;
       }
       if(k>0)
        inorder(root->right, ans ,k);
    }
    int kthSmallest(TreeNode* root, int k) 
    {
        int ans=-1;
        inorder(root, ans,k);
        return ans;
    }
};
