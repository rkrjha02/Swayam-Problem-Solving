#include<bits/stdc++.h>

using namespace std;

int main()
{
    vector<int> preorder;
    int n;
    cout<<"Enter the number of elements of preorder:";
    cin>>n;
    for(int i=0;i<n;i++)
    {
        int x;
        cin>>x;
        preorder.push_back(x);
    }
    int maxi,mini;
    maxi=max(preorder[n-1], preorder[n-2]);
    mini=min(preorder[n-1], preorder[n-2]);
    for(int i=n-3;i>=0;i--)
    {
        if(preorder[i]<mini)
         mini=preorder[i];
        else if(preorder[i]>maxi)
         maxi=preorder[i];
        else
         {
             cout<<"False";
             return 0;
         }
    }
    cout<<"True";
    return 0;
}
