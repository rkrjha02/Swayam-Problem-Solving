class Solution {
public:
    
    vector<int> findSmallestSetOfVertices(int n, vector<vector<int>>& edges) 
    {
       vector<int> degree(n,0);
       for(int i=0;i<edges.size();i++){
           degree[edges[i][1]]++;
       }
       vector<int> answer;
       for(int i=0;i<degree.size();i++){
           if(degree[i]==0)
             answer.push_back(i);
       }
       return answer;
    }
};
